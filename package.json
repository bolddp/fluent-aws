{
  "name": "fluent-aws",
  "version": "0.19.0",
  "description": "Fluent AWS API, written in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "test": "jest --config=./jest.config.json",
    "coverage": "jest --config=./jest.config.json --coverage",
    "npm:publish": "npm run build && npm publish",
    "prebuild": "npm run clean",
    "build": "node ./node_modules/typescript/bin/tsc --sourcemap false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolddp/fluent-aws.git"
  },
  "author": "Daniel Persson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bolddp/fluent-aws/issues"
  },
  "homepage": "https://github.com/bolddp/fluent-aws#readme",
  "devDependencies": {
    "@types/debug": "4.1.4",
    "@types/jest": "29.5.0",
    "@types/node": "20.5.2",
    "@types/node-fetch": "2.5.4",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "jest-stare": "2.5.0",
    "rimraf": "3.0.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "3.395.0",
    "@aws-sdk/client-cloudformation": "3.395.0",
    "@aws-sdk/client-cognito-identity-provider": "3.395.0",
    "@aws-sdk/client-dynamodb": "3.395.0",
    "@aws-sdk/client-ec2": "3.395.0",
    "@aws-sdk/client-ecs": "3.395.0",
    "@aws-sdk/client-iam": "3.395.0",
    "@aws-sdk/client-kms": "3.395.0",
    "@aws-sdk/client-route-53": "3.395.0",
    "@aws-sdk/client-s3": "3.395.0",
    "@aws-sdk/client-sns": "3.395.0",
    "@aws-sdk/client-ssm": "3.395.0",
    "@aws-sdk/client-sts": "3.395.0",
    "@aws-sdk/credential-providers": "3.395.0",
    "@aws-sdk/lib-dynamodb": "3.395.0",
    "@aws-sdk/s3-request-presigner": "3.395.0",
    "@aws-sdk/types": "3.391.0",
    "amazon-cognito-identity-js": "3.2.1",
    "debug": "4.3.2",
    "node-fetch": "2.6.7"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "jestSonar": {
    "reportPath": "test-results",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "junit.xml"
  },
  "jest-stare": {
    "resultDir": "./test-results/jest-stare",
    "reportTitle": "Test results - Fleet device connection service",
    "coverageLink": "../coverage/lcov-report/index.html"
  }
}
